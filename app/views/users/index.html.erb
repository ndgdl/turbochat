<%= turbo_frame_tag "appearance_channel" %>
<div id="appearance_channel"></div>

<div id="chatroom_container" class="row bg-dark text-light">
  <%= render 'layouts/side_panel' %>

  <div id="room_container" class="col-md-8 col-sm-8 bg-dark text-light mt-3">
    <div id="users_list">
      <h4 class="text-center"> All Users </h4>
      <%= turbo_stream_from "users" %>
      <div id="users">
        <%= render @users %>
      </div>
    </div>
  </div>
</div>
